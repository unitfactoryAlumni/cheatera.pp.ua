{
    "name": "cheatera.pp.ua",
    "description": "cheatera.pp.ua",
    "keywords": ["unit factory", "42"],
    "homepage": "https://cheatera.pp.ua/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/omentes/cheatera.pp.ua/issues?state=open",
        "source": "https://github.com/omentes/cheatera.pp.ua"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "klisl/yii2-languages": "^2.4",
        "vlucas/phpdotenv": "^2.5",
        "yiisoft/yii2-authclient": "^2.1",
        "fedemotta/yii2-widget-datatables": "*",
        "kartik-v/yii2-tabs-x": "*",
        "2amigos/yii2-chartjs-widget": "~2.0",
        "kartik-v/yii2-widget-datepicker": "@dev"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
